						<div class="ColumnContents" id="ShowPlayerDemo">
							<script type="text/javascript">
							
							var playerUpdater = {
								
								session: Math.floor(Math.random() * 100000),
								postsId : -1,
								
								load: function() {
									new Ajax.Request("/players/list/?skin=json&callback=playerUpdater.loaded&multi_episode=1&users_id=167079&cachekey=" + this.session);
								},
								
								loaded: function(players) {
									if (players) {
										var select = document.getElementById('ShowPlayers');
										select.options.length = 1;
										var self = this;
										select.onchange = function() { self.getEmbedCode(this.value,self.postsId); };
										for (var i=0; i<players.length; i++) {
											if (players[i].id) {
												select.options[i+1] = new Option(players[i].name,players[i].id);
											}
										}
									}
								},
								
								update_show_player: function() {
									this.load();
								},
								
								getEmbedCode: function() {
									var playersId = $("ShowPlayers").value;
									new Ajax.Request("/players/embed/?posts_id=-1&players_id=" + playersId + "&width="+ $('width').value +"&height="+ $('height').value +
										"&skin=json&callback=playerUpdater.gotEmbedCode&cachekey=" + this.session + "&users_id=167079"
									);
								},
						
								gotEmbedCode: function(params) {
									if (params && params.length && params[0] && params[0].code) {
										var code = params[0].code;
										var textarea = document.getElementById('ShowPlayerCode');
										textarea.value = code;
										$('show_player_embed').innerHTML = code;
									}
									
								}
								
							}
						
							
							</script>
							
							<div id="show_player_embed" style="align: center;"></div>

						</div>

