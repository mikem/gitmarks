<script>

		var permalink = "";

		function getFlashMovie(movieName)
		{
			var isIE = navigator.appName.indexOf("Microsoft") != -1;
			return (isIE) ? window[movieName] : document[movieName];
	 	}
		
		var CurrentPlaylist = [];
		var CurrentItem = 0;

		function getUpdate(type, arg1, arg2)
		{
			//console.log(type + " " + arg1 + " " + arg2);
			switch(type)
			{
				case "complete":
					CurrentItem++;
					updateDisplay();
				break;
				
			}
		}

		function toggleMenu(id) {
			toggleVisibility(id);	
			
			var ml = document.getElementById("MoreLess");
			document.getElementById("AboutMore").blur();
			
			if (ml.src == "http://a.blip.tv/skin/mercury/images/more.gif?v=oldgaryz") {
				ml.src ="http://a.blip.tv/skin/mercury/images/less.gif?v=oldgaryz";
			} else if (ml.src == "http://a.blip.tv/skin/mercury/images/less.gif?v=oldgaryz") {
				ml.src ="http://a.blip.tv/skin/mercury/images/more.gif?v=oldgaryz";
			}
		}
		
		function toggleVisibility(id) {
			var e = document.getElementById(id);
		   if(e.style.display == 'block')
			  e.style.display = 'none';
		   else
			  e.style.display = 'block';
		}

		function playlistUpdate(playlist) {
			//console.log("Playlist Update");
			CurrentPlaylist = playlist;
			updateDisplay();
		}

	 	function updateDisplay()
	 	{
			var item = CurrentPlaylist[CurrentItem];

			document.getElementById('EpisodeTitle').innerHTML = stripHTML(item.title);	
			document.getElementById('EpisodeDescription').innerHTML = stripHTML(item.description);
			
			if (typeof(window.originalTitle) == "undefined") {
				window.originalTitle = document.title || (window.body && window.body.title);
			}
			
			document.title = stripHTML(item.title) + " - " +  window.originalTitle;
			if (window.body) {
				window.body.title = document.title;
			}
	 	}
	 	
		function stripHTML(s) {
			return s.replace(/\\&/g, '&amp;').replace(/\\</g, '&lt;').replace(/\\>/g, '&gt;').replace(/\\t/g, '&nbsp;&nbsp;&nbsp;').replace(/\\n/g, '').replace('<br />', '').replace('<br/>', '').replace('<br>', '').replace('<p>', '').replace('</p>', '');
		}
		
		function toggleSyndicate(syndicate) {
			var bigTheaterContainer = document.getElementById("ShowpageContent");
			var watchingRow = document.getElementById("ShowFooter");
			var syndicateContainer = document.getElementById("SyndicateContainer");
			var syndicateLink = document.getElementById("SyndShow");
			var watchShowLink = document.getElementById("WatchShow");
		
			if (syndicate) {
				if (navigator.appName == "Microsoft Internet Explorer") {
					bigTheaterContainer.style.display = '';
					watchingRow.style.display = '';
					syndicateLink.style.display = '';
				} else {
					bigTheaterContainer.style.display = "inherit";
					watchingRow.style.display = "inherit";
					syndicateLink.style.display = "inherit";
				}
				syndicateContainer.style.display = "none";
				watchShowLink.style.display = "none";
			} else {
				bigTheaterContainer.style.display = "none";
				watchingRow.style.display = "none";
				syndicateContainer.style.display = "block";
		 		watchShowLink.style.display = "block";
				syndicateLink.style.display = "none";
				playerUpdater.update_show_player();
			}
		}
			
		function checkSyndicateHash () {
			
			if(location.hash == "#syndicate") {
				toggleSyndicate(false);
			}
			
		}
		
		Pokkari.AttachEvent(window,"load",function() { checkSyndicateHash(); });		
</script>

