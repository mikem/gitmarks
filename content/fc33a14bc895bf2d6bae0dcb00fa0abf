			<script type="text/javascript">
				var so;
				var customPlayerId = "-2";
				var backcolor;
				var frontcolor;
				var lightcolor;
				function EmbedCustomPlayer(params) {
					// The custom player code will come back with the extra params.  Drop them.
					var url = new Url(params[0]);
					url.removeQueryParam('view');
					url.removeQueryParam('callback');
					url.removeQueryParam('skin');

					backcolor=url.getQueryParam('backcolor');
					frontcolor=url.getQueryParam('frontcolor');
					lightcolor=url.getQueryParam('lightcolor');
					// if the players colors are customized then we reset teh style of the surrounding html to match colors
					if(backcolor){

						document.getElementById("ShowpageContent").style.background="#"+backcolor.substring(2)+" url(\"http://a.blip.tv/skin/mercury/images/showpage_theater_top.gif?v=oldgaryz\") no-repeat";
						document.getElementById("ShowpageContentFooter").style.background="#"+backcolor.substring(2)+" url(\"http://a.blip.tv/skin/mercury/images/showpage_theater_bot.gif?v=oldgaryz\") no-repeat";
						document.getElementById("ShowpageContentFooter").style.backgroundPosition="bottom";
						document.getElementById("ShowpageContentFooter").style.borderTop="1px solid #"+backcolor.substring(2);
					}
					if(frontcolor){
						document.getElementById("ShowpageContentFooter").style.foreground="#"+frontcolor.substring(2)+" url(\"http://a.blip.tv/skin/mercury/images/showpage_theater_bot.gif?v=oldgaryz\") no-repeat";
					}
					if(lightcolor){
						document.getElementById("ShowpageContentFooter").style.lightcolor="#"+lightcolor.substring(2)+" url(\"http://a.blip.tv/skin/mercury/images/showpage_theater_bot.gif?v=oldgaryz\") no-repeat";
					}


					// Support for loading enhancements
					url.setQueryParam('staggeredLoad',true);

					var fileUrl = new Url(url.getQueryParam('file'));
					fileUrl.setQueryParam('use_direct',1);
					fileUrl.setQueryParam('use_documents',1);
					url.setQueryParam('file',fileUrl.getUrl());


					// Hack in Stratos
					//url.setQueryParam('listOnStart',true);
					url.setQueryParam('autoplay',true);
					if (!Boolean(url.getQueryParam('useoldplayer'))) {
						url.updateUrl();
						url.url = url.url.replace('showplayer.swf','stratos.swf');
					}

					var so = new SWFObject(url.getUrl(), "showplayer1", "860", "513", "8", "#000000");
					so.addParam("allowfullscreen", "true");
					so.addParam("allowScriptAccess", "always");
					so.addParam("wmode", "opaque");
					so.write("ShowpageShowplayer");
				}

				if (customPlayerId > 0) {
					new Ajax.Request("/play/?users_id=167079&players_id=-2&posts_id=-2&skin=json&autoplay=true&view=url&callback=EmbedCustomPlayer");
				}
				else {
					so = new SWFObject("http://a.blip.tv/scripts/flash/stratos.swf?autostart=true&backcolor=0x000000&lightcolor=0xAAAAAA&frontcolor=0x999999&enablejs=true&autoplay=true&file=http://clojure.blip.tv/rss/flash/%3Freferrer=clojure.blip.tv%26source=1%26use_direct=1%26use_documents=1&staggeredLoad=true", "showplayer1", "860", "513", "8", "#000000");
					so.addParam("allowfullscreen", "true");
					so.addParam("allowScriptAccess", "always");
					so.addParam("wmode", "opaque");
					so.write("ShowpageShowplayer");

				}
			</script>
			
		
