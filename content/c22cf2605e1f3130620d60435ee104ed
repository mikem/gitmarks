$(document).ready(function ($) {
	// Rotating home page splashes
	var splashes = $("#rotatingSplashes > div.rotatingSplash");
	var len = splashes.length;
	if (len > 1) {
		var randomNum1 = Math.floor((Math.random() * len));
		var randomNum2 = Math.floor((Math.random() * len));
		while(randomNum2 == randomNum1) {
			randomNum2 = Math.floor((Math.random() * len)); 
		}
		var splash1 = splashes.eq(randomNum1);
		var splash2 = splashes.eq(randomNum2);
		$("#rotatingSplashes").prepend(splash1.css("display", "block")).prepend(splash2.css("display", "block"));
	} else {
		splashes.css("display", "block");
	}
});
