		<SCRIPT TYPE="text/javascript">
		var checkflag = "true";
		function check(field) {
		if (checkflag == "false") {
		for (i = 3; i < 69 ; i++) {
			field[i].checked = true;
		}
		checkflag = "true";
		return "Uncheck All"; }
		else {
  		for (i = 3; i < 69 ; i++) {
			field[i].checked = false;
		}
		checkflag = "false";
		return "  Check All  "; }
		}
