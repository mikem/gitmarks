  function share(playlist_encrypted_id)
  {
    var fs = window.open( "/share?p=" + playlist_encrypted_id,
            "Share", "toolbar=no,width=546,height=485,status=no,resizable=yes,fullscreen=no,scrollbars=no");
    fs.focus();
  }
