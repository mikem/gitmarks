function addToFavorites() {
    // Mozilla Firefox Bookmark
    if (window.sidebar) {
        window.sidebar.addPanel('koolweb2','http://www.koolweb2.com',"");
    // IE Favorite
    } else if( window.external ) {
        window.external.AddFavorite( 'http://www.koolweb2.com', 'koolweb2');
    // Opera Hotlist
    } else if(window.opera && window.print) {
        return true;
    }
}


</script>


