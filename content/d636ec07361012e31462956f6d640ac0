<script type='text/javascript'>
function _splitPost(divId) {
  var span = _findSpan(document.getElementById(divId));
  if (span) {
    span.style.display = "none";
    return true;
  }
}
function _findSpan(el) {
  if (el.tagName == "SPAN" && el.className == "" && el.id == "") return el;
  for (var i = 0; i < el.childNodes.length; i++) {
    var span = _findSpan(el.childNodes[i]);
    if (span) return span;
  }   
}
</script>

