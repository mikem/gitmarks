<script style="text/javascript">
function toggleArchive(elm) {
	if (elm.parentNode.className == 'collapsed') {
		elm.parentNode.className = 'expanded';
		elm.innerHTML = '&#9660;';
	} else {
		elm.parentNode.className = 'collapsed';
		elm.innerHTML = '&#9658;';
	}
}

function toggleEntries(elm, monthName, year, ignoreToggle) {

	if (!ignoreToggle) {
		toggleArchive(elm);
	}
	
	//el = elm.parentNode.getElementsByClassName('entriesList')[0];
	
	var childnodes = elm.parentNode.childNodes;
	
	for (var i=0; i<childnodes.length;i++) {
		if (childnodes[i].className && childnodes[i].className.indexOf('entriesList') != -1) {
			el = childnodes[i];
			break;
		}
	}
	
	if (el.className && el.className.indexOf('loaded') == -1) {
		// load and at the end, add the "loaded" classname
		var params = 'year=' + year + '&monthName=' + monthName;
		
		
			params += '&blog=094d7399ff3efb2000ff425b10e5066a';
		
		
		new Ajax.Request('/blogbuilder/admin/showEntryTitlesForMonth.spr',
			{
				method: 'get',
				parameters: params,
				onComplete: function(response) {
					el.innerHTML = response.responseText;
					el.className = el.className += ' loaded';
				}
			}
		);
	}
}
</script>
	



	
	
		






