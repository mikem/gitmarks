	<h2>Blog archive</h2>

<div class="innerSidebarContent" id="blogArchive_094d73990c14a6c2010c14a78cf5080b">

<img src="/blogbuilder/media/images/popup/tab-spinner.gif" /> Loading&hellip;

<script type="text/javascript">
Event.observe(window,'load', function() {
	new Ajax.Updater(
		'blogArchive_094d73990c14a6c2010c14a78cf5080b',
		'/blogbuilder/admin/showArchive.spr',
		{
			asynchronous: true,
			method: 'get',
			parameters: 'blog=094d7399ff3efb2000ff425b10e5066a',
			evalScripts: true
		}
	);
});
</script>

</div>
</div>
