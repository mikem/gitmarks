<script type="text/javascript">
	$(function() {
		if($('#disqus_thread').position()) {
		var bar = $('#moreSlideContainer');
		var trigger = $('#disqus_thread').position().top - $(window).height();
		$(window).scroll(function() {
			if($(this).scrollTop() > trigger) {
				bar.stop().animate({'right' : '0px'}, 300);
			} else {
				bar.stop().animate({'right' : '-435px'}, 300);
			}
		});
		}
	});
	
	
/**
 * We use the initCallback callback
 * to assign functionality to the controls
 */
function mycarousel_initCallback(carousel) {
    jQuery('#flycarousel-next').bind('click', function() {
    	carousel.startAuto(0);
        carousel.next();
        return false;
    });

    jQuery('#flycarousel-prev').bind('click', function() {
    	carousel.startAuto(0);
        carousel.prev();
        return false;
    });
    
    // Pause autoscrolling if the user moves with the cursor over the clip.
    carousel.clip.hover(function() {
        carousel.stopAuto();
    }, function() {
        carousel.startAuto();
    });

};

// Ride the carousel...
jQuery(document).ready(function() {
    jQuery("#flycarousel").jcarousel({
        scroll: 1,
        auto: 13,
        wrap: 'circular',
        initCallback: mycarousel_initCallback,
        // This tells jCarousel NOT to autobuild prev/next buttons
        buttonNextHTML: null,
        buttonPrevHTML: null
    });
});

	
</script>



