    <script type="text/javascript">

       function rate(rating, message, name, token)  {

           var f = $j('#rate-form');

           f.append('<input type="hidden" name="rating" value="'+ rating +'"/>');

           f.append('<input type="hidden" name="jive.token.name" value="'+ name +'"/>');

           f.append('<input type="hidden" name="'+ name +'" value="'+ token +'"/>');

           f.append('<input type="hidden" name="message" value="'+ message +'"/>');

           f.attr('action', "/msgrate.jspa");

           f.submit();

           return false;

       }

   </script>












