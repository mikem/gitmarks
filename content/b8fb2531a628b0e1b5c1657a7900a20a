$(document).ready(function() {
   
   $("#languageDropDownBox a").css({visibility:'hidden'});
   $("#languageDropDownBox").css({opacity:0});

   $("#selectedLanguage").click(
   function() {
	$("body").unbind("click");
	$("#languageDropDownBox a").css({visibility:'visible'});
	$("#languageDropDownBox").animate({opacity:1},250, null, function() 
	{
		$("body").click(
    			function() {
					$("#languageDropDownBox").animate({opacity:0});
					$("#languageDropDownBox a").css({visibility:'hidden'});
		 			$("body").unbind("click")
 				  })
	});

	//$("#languageDropDownBox a").click(function() { return false; });
   })	
