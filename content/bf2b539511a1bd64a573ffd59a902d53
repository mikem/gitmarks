    function htmlFacebook(){
        var tagHTML = document.getElementsByTagName("head")[0].parentNode;
        tagHTML.setAttribute("xmlns", "http://www.w3.org/1999/xhtml");
        tagHTML.setAttribute("xmlns:fb", "http://www.facebook.com/2008/fbml");
        tagHTML.setAttribute("xml:lang","en");
        tagHTML.setAttribute("lang","en");        
    }
  
