    function checkClient(){
        if((navigator.userAgent.match(/iPhone/i))||
           (navigator.userAgent.match(/iPod/i))) {
    	      fullURL = parent.document.URL
            query = fullURL.substring(fullURL.indexOf('?')+3, fullURL.length)
            //if (!query) document.location.href='http://www.tissot.ch/iphone';
        }else{
            
        }
    }
    
