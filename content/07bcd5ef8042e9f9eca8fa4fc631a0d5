<script type="text/javascript">
		function createIframe(src, width, height)
		{
			var altIframe;
			try
			{
				altIframe = document.createElement("<\iframe marginheight='0' marginwidth='0'/>");
			}
			catch (e)
			{
				altIframe = document.createElement("iframe");
				altIframe.setAttribute("marginwidth", 0);
				altIframe.setAttribute("marginheight", 0);
			}
			altIframe.src = src;
			altIframe.frameBorder = 0;
			altIframe.scrolling = "no";
			altIframe.width = width;
			altIframe.height = height;
			
			return altIframe;
		}

		function myNoAdCallback(div)
		{
			div.appendChild(createIframe("http://armorgames.com/misc/videoEgg/default/ctc-iphone.html", 728, 90));
		}

		var ve_publisher = "armor";
