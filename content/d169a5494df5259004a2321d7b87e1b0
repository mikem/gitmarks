  //<![CDATA[
    $(document).ready(function () { 
      var basicSearchSuggest = APP(SearchSuggest, {
        seed:  '#page-header .search', 
        input: '#page-header .search input:first'
      });
      basicSearchSuggest.start();
    
      var s = COOKIES.getCookie('s');
      if (s) {
        $('#releaseInfo').html(s.replace(/\+/g, ' '));
        $('#page-footer').addClass('with-release');
      }
    });
  //]]>
</script>
