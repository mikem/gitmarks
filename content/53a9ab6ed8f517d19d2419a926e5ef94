		case "Articles/ShiftAltCtrl/index.html": finalurl = finalurl + "category/ShiftAltCtrl/"; break; 
		case "Articles/ShiftAltCtrl/shiftaltctrl.html": finalurl = finalurl + "category/ShiftAltCtrl/"; break; 
		case "Articles/ShiftAltCtrl/WhereEveryonesaPig.html": finalurl = finalurl + "category/ShiftAltCtrl/"; break; 		
		default: finalurl = "http://www.agilemanagement.net"; break;
	}
	
	//redirecting:
	window.location.replace(finalurl);
	}
	
	window.onload = redirect;
</script>
</HEAD>
<BODY></BODY></HTML>


