	<div class="region">
	<script language="JavaScript" type="text/javascript">
				function Set_Cookie(name, value){
					if (value == null) value = "";
						var theCookie = name + "=" + value + "; path=/; domain=gamesradar.com";
						document.cookie=theCookie;
					}
				function cleanURL(){
					var cleanUrl = parent.document.URL;

					if( cleanUrl.indexOf("setRegion") != -1){
						cleanUrl = cleanUrl.substring( 0, cleanUrl.indexOf("setRegion")-1);
						if(cleanUrl.charAt(cleanUrl.length - 1) == "&") { cleanUrl = cleanUrl.substring(0, cleanUrl.length - 2); }
						if(cleanUrl.charAt(cleanUrl.length - 1) == "?") { cleanUrl = cleanUrl.substring(0, cleanUrl.length - 2); }
					}
					if( cleanUrl.indexOf("#") != -1){
						if( cleanUrl.indexOf("?") != -1){
							cleanUrl = cleanUrl.replace('#', '&1#');
						} else {
							cleanUrl = cleanUrl.replace('#', '?#');
						}
					}
					return cleanUrl;
				}
				function switchLocale(region){
					var cleanUrl = cleanURL();
					Set_Cookie("grRegion", region);
					/*
					var addParam;
					if( cleanUrl.indexOf("?") == -1){
						addParam = "?setRegion="+region;
					}else{
						addParam = "&setRegion="+region;
					}

					cleanUrl += addParam;
					*/
					location.replace(cleanUrl);
				}
			</script>

		<h3 class="select_region">Select Region:</h3>
		<ul class="region_list">
		
		
			<li class="us"><a title="US" class="default_flag">United States</a></li>
			<li class="uk"><a title="UK" class="switch_flag" onClick="javascript: switchLocale(&#34;uk&#34;)">United Kingdom</a></li>
		
		
		
		</ul>
