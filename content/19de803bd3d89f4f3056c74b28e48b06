$(function(){
$(".topnav li .dir").hover(function() {
	$(this).css("background","none");$(this).css("background-color","white");$(this).css("color","black");$(this).css("text-decoration","underline");
	$(this).parent().find("ul.subnav").show();
	$(this).parent().hover(function() {
	}, function(){
		$(this).parent().find("ul.subnav").hide();
           $(this).find("a").css("background","none");$(this).find("a").css("background-color",null);$(this).find("a").css("color","white");$(this).find("a").css("text-decoration","none");
           if($(this).attr("id") == "active") {
               $(this).find("a").css("background","transparent url(/images/gr-top-nav-active-left.png) no-repeat top left");
           }            
	});
}, function() {});
});
</script>
