 <script src="http://static.ak.connect.facebook.com/js/api_lib/v0.4/FeatureLoader.js.php" type="text/javascript"></script>
 <script type="text/javascript">
  FB.init("2f0593d69fee06c60819a2d2c140f7a6", "/xd_receiver.htm");

  function facebook_callback_none() {
  }
  
  function facebook_requireSession() {
      FB.ensureInit(
    	       function() {
    	        FB.Connect.requireSession(facebook_onlogin);
    	       }
    	      );
  }
  
  function facebook_requireSessionWithPerm(ext_perm) {
      FB.ensureInit(
        function() {
            FB.Connect.requireSession(
              function() {
      			FB.Facebook.apiClient.users_hasAppPermission(
      					ext_perm, function(response) {
      						if (response != 1) {
      							FB.Connect.showPermissionDialog(ext_perm,
      	      						function(perms) {
      								  facebook_onlogin();
      								});
      							} else {
      						        var url = "/radarnation/register";
      						        parent.location.replace(url);
      							}
      					});
              }
            )
    	}
      );
  }
  
  function facebook_onlogin() {
      FB.ensureInit(
       function() {
    	 try {
             var grReferer = getOrigRequestURI('http://www.gamesradar.com');
             Set_GRCookie('grRefererCookie', grReferer, 15, false);
         } catch (err0) { }
    	 var uid = FB.Connect.get_loggedInUser();
    	 var url = "/radarnation/login-facebook?uid=" + uid;
    	 parent.location.replace(url);
       }
      );
  }
  
  function facebook_onlink() {
      FB.ensureInit(
       function() {
         try {
             var grReferer = getOrigRequestURI('http://www.gamesradar.com');
             Set_GRCookie('grRefererCookie', grReferer, 15);
         } catch (err0) { }
         var uid = FB.Connect.get_loggedInUser();
         var url = "/radarnation/login-facebook?uid=" + uid;
         window.location = url;
       }
      );
  }
  
  function facebook_logout() {
      FB.ensureInit(
       function() {
        FB.Connect.logout(facebook_callback_none);
       }
      );
  }
  
  function facebook_logout(callback) {
	  FB.ensureInit(
	   function() {
		 var session = FB.Facebook.apiClient.get_session();
		 if (!session) {
			 parent.location.replace("/radarnation/logout");
		 } else {
          FB.Connect.logout(callback);
		 }
	   }
	  );
  }
  
  function is_facebook_logged() {
      FB.ensureInit(
       function() {
         var session = FB.Facebook.apiClient.get_session();
         if (!session) {
             return false;
         } else {
             return true;
         }
       }
      );
      return false;
  }
 </script>




