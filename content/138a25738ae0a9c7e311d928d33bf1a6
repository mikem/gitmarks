<script type="text/javascript" src="http://i.investopedia.com/inv/092009/home/js/iframefix.js">;</script>

<script type="text/javascript" language="javascript">
        $(document).ready(function(){
            var loginUrl = 'http://join.investopedia.com/login/login.aspx?ReturnUrl=';
            var logoutUrl = 'http://join.investopedia.com/login/logout.aspx?ReturnUrl=';
            var userProfileUrl = 'http://www.investopedia.com/Email/userprofile.aspx';
            var currUrl = window.top.location.href;
            if (currUrl.indexOf('localhost')!=-1) {
                loginUrl = 'http://localhost:1760/login/login.aspx?ReturnUrl=';
                logoutUrl = 'http://localhost:1760/login/logout.aspx?ReturnUrl=';
                userProfileUrl = 'http://localhost:1760/Email/userprofile.aspx';
            }
            if (currUrl.indexOf('maintenance.invlocal')!=-1) {
                loginUrl = 'http://join.maintenance.invlocal.com/login/login.aspx?ReturnUrl=';
                logoutUrl = 'http://join.maintenance.invlocal.com/login/logout.aspx?ReturnUrl=';
                userProfileUrl = 'http://investopedia.maintenance.invlocal.com/Email/userprofile.aspx';
            }
            if (currUrl.indexOf('teststage.invlocal')!=-1) {
                loginUrl = 'http://join.teststage.invlocal.com/login/login.aspx?ReturnUrl=';
                logoutUrl = 'http://join.teststage.invlocal.com/login/logout.aspx?ReturnUrl=';
                userProfileUrl = 'http://investopedia.teststage.invlocal.com/Email/userprofile.aspx';
            }
            if (currUrl.indexOf('invwf')!=-1) {
                loginUrl = 'http://join.livestage.invwf.com/login/login.aspx?ReturnUrl=';
                logoutUrl = 'http://join.livestage.invwf.com/login/logout.aspx?ReturnUrl=';
                userProfileUrl = 'http://investopedia.livestage.invwf.com/Email/userprofile.aspx';
            }
            $('#ctl00_ctl00_SignIn').attr('href',(loginUrl + currUrl));
            $('#ctl00_ctl00_Logout').attr('href',logoutUrl + currUrl);
            $('#ctl00_ctl00_aUsername').attr('href',userProfileUrl);
         });
</script>

