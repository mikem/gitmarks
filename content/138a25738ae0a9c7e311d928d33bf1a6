    <script type="text/javascript" language="javascript">
        $(document).ready(function(){
        
            // set the default link paths
            var loginUrl = 'http://join.investopedia.com/login/login.aspx?ReturnUrl=';
            var logoutUrl = 'http://join.investopedia.com/login/logout.aspx?ReturnUrl=';
            var registerUrl = 'http://join.investopedia.com/investopedia/join1?RegLink=Investopedia_Header&src=7&ReturnUrl=';
            var currUrl = window.top.location.href;
            
            if (currUrl.indexOf('localhost') >= 0) {
                loginUrl = 'http://localhost:1760/login/login.aspx?ReturnUrl=';
                logoutUrl = 'http://localhost:1760/login/logout.aspx?ReturnUrl=';
                registerUrl = 'http://localhost:1760/investopedia/join1?RegLink=Investopedia_Header&src=7&ReturnUrl=';
            }
            else if (currUrl.indexOf('maintenance.invlocal') >= 0) {
                loginUrl = 'http://join.maintenance.invlocal.com/login/login.aspx?ReturnUrl=';
                logoutUrl = 'http://join.maintenance.invlocal.com/login/logout.aspx?ReturnUrl=';
                registerUrl = 'http://join.maintenance.invlocal.com/investopedia/join1?RegLink=Investopedia_Header&src=7&ReturnUrl=';
            }
            else if (currUrl.indexOf('teststage.invlocal') >= 0) {
                loginUrl = 'http://join.teststage.invlocal.com/login/login.aspx?ReturnUrl=';
                logoutUrl = 'http://join.teststage.invlocal.com/login/logout.aspx?ReturnUrl=';
                registerUrl = 'http://join.teststage.invlocal.com/investopedia/join1?RegLink=Investopedia_Header&src=7&ReturnUrl=';
            }
            else if (currUrl.indexOf('invwf') >= 0) {
                loginUrl = 'http://join.livestage.invwf.com/login/login.aspx?ReturnUrl=';
                logoutUrl = 'http://join.livestage.invwf.com/login/logout.aspx?ReturnUrl=';
                registerUrl = 'http://join.livestage.invwf.com/investopedia/join1?RegLink=Investopedia_Header&src=7&ReturnUrl=';
            }
            
            // set the links
            $('#ctl00_ctl00_SignIn').attr('href', (loginUrl + currUrl));
            $('#ctl00_ctl00_Logout').attr('href', (logoutUrl + currUrl));
            $('#ctl00_ctl00_Register').attr('href', (registerUrl + currUrl));
         });
    </script>

