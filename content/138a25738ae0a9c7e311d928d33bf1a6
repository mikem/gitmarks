    <script type="text/javascript">
	function openSite(thisELE)
	{
	var index = thisELE.selectedIndex;
	
	if(index > 0)
	{
	window.location = thisELE.options[index].value;
	}
	}
    </script>
 



    <script type="text/javascript">
        
    jQuery(document).ready(function() {
        jQuery("#ctl00_ctl00_txtTopSearch").click(function() {
            if (jQuery(this).val() == 'keyword')
                jQuery(this).val('');
        });
        jQuery("#ctl00_ctl00_txtTopQuoteSearch").click(function() {
            if (jQuery(this).val() == 'symbol')
                jQuery(this).val('');

        });
        jQuery("#ctl00_ctl00_btnTopSearch").click(function() {
            var query = jQuery("#ctl00_ctl00_txtTopSearch").val();
            
            if (query == 'keyword' || query == '')
                return false;

        });        
    });

    </script>
      <script type="text/javascript">
sfHover = function() {
	var sfEls = document.getElementById("nav").getElementsByTagName("LI");
	for (var i=0; i<sfEls.length; i++) {
		sfEls[i].onmouseover=function() {
			this.className+=" sfhover";
		}
		sfEls[i].onmouseout=function() {
			this.className=this.className.replace(new RegExp(" sfhover\\b"), "");
		}
	}
}
if (window.attachEvent) window.attachEvent("onload", sfHover);

    </script>
