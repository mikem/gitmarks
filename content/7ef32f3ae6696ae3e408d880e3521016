function hideShowIframe(_param)
{
    document.getElementById(_param.name).style.display=_param.display;
    if(_param.display == "none"){
        switch(_param.name) {
            case "adw_1" : 
                document.getElementById("div_adw_1").style.width=0;
                document.getElementById("div_adw_1").style.height=0;
                document.getElementById("div_adw_1" ).style.top=2000;
                document.getElementById("div_adw_1" ).style.left=4000;
                break ;
            case "adw_2" : 
                document.getElementById("div_adw_2").style.width=0;
                document.getElementById("div_adw_2").style.height=0;
                break ;
            case "adsense" : 
                document.getElementById("div_adw_3").style.width=0;
                document.getElementById("div_adw_3").style.height=0;
                break ;
        }
    }

    if(_param.display == "block"){
        switch(_param.name) {
            case "adw_1" : 
                document.getElementById("adw_1").style.width=300;
                document.getElementById("adw_1").style.height=250;
                document.getElementById("div_adw_1").style.width=300;
                document.getElementById("div_adw_1").style.height=250;
                document.getElementById("div_adw_1" ).style.top=_param.y;
                document.getElementById("div_adw_1" ).style.left=_param.x;
                break ;
            case "adw_2" : 
                document.getElementById("adw_2").style.width=728;
                document.getElementById("adw_2").style.height=90;
                document.getElementById("div_adw_2").style.width=728;
                document.getElementById("div_adw_2").style.height=90;
                break ;
            case "adsense" : 
                document.getElementById("adsense").style.width=735;
                document.getElementById("adsense").style.height=100;
                document.getElementById("adsense").src="pub.html"
                document.getElementById("div_adw_3").style.width=735;
                document.getElementById("div_adw_3").style.height=100;
                break ;
        }
    }
}



</script>
