<script type="text/javascript">
          function getSuccessUrl() {
             var shouldExclude = false;
             var successUrl = "";
              if(!shouldExclude){
                 if(document.referrer){
                     if(document.referrer.indexOf("successURL") != -1){
                        successUrl = document.referrer.substring(document.referrer.indexOf("successURL"), document.referrer.length - 1);
                     }else{
                         var lastwack = document.referrer.lastIndexOf("/");
                         successUrl = "/" +  document.referrer.substring(lastwack + 1,document.referrer.length);
                     }
                 }

                 if((successUrl == "") || (successUrl.indexOf("login") != -1) || (successUrl.indexOf("logout") != -1)){
                       successUrl = "/index.jspa";
                 }
               document.location = "https://support.apple.com/cgi-bin/WebObjects/ACAuthWeb.woa/wa/login?appIdKey=2ddbca23a85d20e5bf7478812379ae23&path=/login.jspa" + escape("?successURL=" +  successUrl);
             }else{
                document.location = "https://support.apple.com/cgi-bin/WebObjects/ACAuthWeb.woa/wa/login?appIdKey=2ddbca23a85d20e5bf7478812379ae23&path=/login.jspa" + escape("?successURL=/index.jspa");
             }
          }
        </script>


