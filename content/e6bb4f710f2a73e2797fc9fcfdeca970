<h2 class='title'>KISSmetrics</h2>
<div class='widget-content'>
<div id='pwd_by_km'>KISSmetrics is loading...</div>
<script type="text/javascript">

function addActionByClassName(className, actionName, actionType) {
   var elems = document.getElementsByClassName(className);
   for (var i = 0; i <= elems.length - 1; i++) {
     addActionByObj(elems[i], actionName, actionType);
   }
}

function addActionByObj(obj, actionName, actionType) {
  if( obj && obj.addEventListener ) {
    obj.addEventListener(actionType, 
       function() { _kmq.push(['record', actionName]); },
       false);
  }
}

function setOnclickById(elementId, actionName) {

  _kmq.push(['trackClick', elementId, actionName]);
}

function set_cookie ( name, value, exp_y, exp_m, exp_d, path, domain, secure )
{
  var cookie_string = name + "=" + escape ( value );

  if ( exp_y )
  {
    var expires = new Date ( exp_y, exp_m, exp_d );
    cookie_string += "; expires=" + expires.toGMTString();
  }

  if ( path )
        cookie_string += "; path=" + escape ( path );

  if ( domain )
        cookie_string += "; domain=" + escape ( domain );
  
  if ( secure )
        cookie_string += "; secure";
  
  document.cookie = cookie_string;
}

function get_cookie ( cookie_name )
{
  var results = document.cookie.match ( '(^|;) ?' + cookie_name + '=([^;]*)(;|$)' );

  if ( results )
    return ( unescape ( results[2] ) );
  else
    return null;
}

function setupKT() 
{

  _kmq.push(function(){
    // Set up the experiment (this is the meat and potatoes)

var subscribeText = KM.ab('Subscribe Label Text 3-way', ['Subscribe now', 'Subscribe today', "Please subscribe"]);
var subscribeButton = document.getElementById('sll_subscribe_button');
subscribeButton.value = subscribeText;
subscribeButton.onclick = function()
{ 
  var email_name_box = document.getElementById('sll_subscribe_email');
  var email_name = email_name_box.value;
  _kmq.push(['identify', email_name]);
} 

  });





addActionByObj(document, "Clicked anything", "click");



addActionByClassName("star-span", "Clicked a star rating (outbrain)", "click");
addActionByClassName("post-labels", "Clicked a post label", "click");
addActionByClassName("feedburnerFlareBlock", "Clicked a feedburner flare link", "click");
addActionByClassName("widget Subscribe", "Clicked blogger subscribe widget", "click");
addActionByClassName("amzn_wdgt_pad", "Clicked amazon widget", "click");
addActionByClassName("widget-content", "Clicked any widget", "click");
addActionByClassName("tweetmeme_div", "Clicked retweet link");




setOnclickById('sll_feedcount_link', "Clicked Feedcount");
setOnclickById('things_to_buy', "Clicked a Thing to buy");

var retweetEnabled = 'Enabled';
if(retweetEnabled == 'Enabled') {
 var rtwidgets = document.getElementsByClassName("tweetmeme_div");
 for(var i = 0; i < rtwidgets.length; i++ ) {
    var rtwidget = rtwidgets[i];
    rtwidget.style.display = "block";
 }
}


var thingsToBuyVisible = 'Visible';

if( thingsToBuyVisible == "Visible" ) {
  var ttbDiv = document.getElementById("things_to_buy");
  if( ttbDiv ) {
    ttbDiv.style.display = "block";
  }
}


var subscribeStyle = 'Andrew Chen Style';
if( subscribeStyle == "Andrew Chen Style" ) {
  var subWidget = document.getElementById("HTML1");
  var subTitle = subWidget.getElementsByTagName("h2")[0];
  subTitle.innerHTML = "Get new essays by email (1-2x/week)";

 var posts = document.getElementsByClassName("post-body entry-content");
 for(var i = 0; i < posts.length; i++ ) {
    var post = posts[i];
    var wantmore = document.createElement("div");
    wantmore.innerHTML = '<p><b>Want more?</b><br>If you liked this post, please <a href="http://feeds.feedburner.com/startup/lessons/learned">subscribe</a> or <a href="http://twitter.com/ericries">follow me on Twitter</a>.';
    addActionByObj(wantmore, "Clicked Andrew Chen want-more", "click");
    post.appendChild(wantmore);
 }


}




var first_visit = get_cookie("first_visit");
var now = new Date();
if( first_visit ) {
  _kmq.push(['record', 'Repeat visit']);
  var first_date = new Date(first_visit);
  var elapsed = now - first_date;
  if( elapsed > 3*1000*60*60*24 ) {
    _kmq.push(['record','Repeat visit > 3 days later']);
  }  
  if( elapsed > 7*1000*60*60*24 ) {
    _kmq.push(['record','Repeat visit > 7 days later']);
  }  
} else {
  set_cookie("first_visit", now.toUTCString(), 2020, 01, 01, "", "startuplessonslearned.com", false);
}

_kmq.push(function(){
var kmdiv = document.getElementById('pwd_by_km');
kmdiv.innerHTML = 'You are being watched. But in a good way. Powered by <a href="http://kissmetrics.com">KISSmetrics</a>.';
});
}

setupKT();
</script>
