            name=johan                                  created    name=D


                             follows

                                        name=peter                ./outE[@label=ʻfollowsʼ]/inV
 </li><li>Local Search: “What webpages did Emil’s followers
                   create?”
                                        name=tobias

                             follows


                                                        created      name=C
             name=alex                      3
                              created
