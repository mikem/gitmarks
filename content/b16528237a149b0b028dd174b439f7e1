<script type="text/javascript">
  // A shorthand for links to timeanddate.com
  GCJ.timeAndDateLink = function(year, month, day, hour, minute) {
    window.location = [
      "http://www.timeanddate.com/worldclock/fixedtime.html?p1=0",
      "year=" + year,
      "month=" + month,
      "day=" + day,
      "hour=" + hour,
      "min=" + minute,
      "sec=0"
    ].join("&");
  };

  /** Aligns column widths in two or more tables. */
  GCJ.linkTablesVertically = function(classLabel) {
    // Find all HTML tables.
    var allTables = document.getElementsByTagName("table");

    // Select the ones that have the given class label.
    var tables = [];
    for (var i = 0; i < allTables.length; i++) {
      if (allTables[i].getAttribute("class") == classLabel) {
        tables.push(allTables[i]);
      }
    }
    if (tables.length == 0) {
      return;
    }

    // Get the list of columns of each table.
    var columns = [];
    for (var i = 0; i < tables.length; i++) {
      var firstRow = tables[i].getElementsByTagName("tr")[0];
      columns.push(firstRow.getElementsByTagName("td"));
    }

    // Compute the optimal width of each column.
    var columnWidths = [];
    for (var j = 0; j < columns[0].length; j++) {
      var myWidth = 0;
      for (var i = 0; i < columns.length; i++) {
        myWidth = Math.max(myWidth, columns[i][j].offsetWidth);
      }
      columnWidths.push(myWidth);
    }

    // Set the widths to their new values.
    for (var i = 0; i < tables.length; i++) {
      for (var j = 0; j < columns[0].length; j++) {
        // Figure out the magic difference between width and offsetWidth.
        columns[i][j].width = columnWidths[j];
        var delta = columns[i][j].offsetWidth - columnWidths[j];

        // Now set the width such that offsetWidth is what we want it to be.
        columns[i][j].width = columnWidths[j] - delta;
      }
    }
  };

  GCJ.main = function() {
    GCJ.renderNavPanel();
    GCJ.linkTablesVertically("schedule");
    GCJ.linkTablesVertically("results");
  };
</script>

