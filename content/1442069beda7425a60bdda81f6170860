    function isNotEmpty(viewTranscript)     
    {        
    if (trim(viewTranscript.pid.value) == "")       
     {           
      alert("Please enter a value for the \"Transcript\" field.");          
        viewTranscript.pid.focus();           
         return (false);       
          }        
          else        
