             function 
             isNumeric(viewTranscript)    
             {          
             var checkOK   = "0123456789-";        
             var checkStr  = viewTranscript.pid.value;        
             var allValid  = true;       
              var decPoints = 0;        
              var allNum    = "";        
              for (i = 0;  i < checkStr.length;  i++)        
              {            ch = checkStr.charAt(i);            
              for (j = 0;  j < checkOK.length;  j++)                
              if (ch == checkOK.charAt(j))                    
              break;            
              if (j == checkOK.length)            
              {                
              allValid = false;                
              break;            
              }            
              allNum += ch;        
              }        
              if (!allValid)        
              {            
              alert("Please enter only digits in the \"Transcript\" field.");            
              viewTranscript.pid.focus();            
              return (false);       
              }        
              else        
              {            
              return (true);       
              }    
              } 
              function trim(theValue)     
              {        
              while('' + theValue.charAt(theValue.length - 1) == ' ')            
              theValue = theValue.substring(0, theValue.length - 1);            
              while('' + theValue.charAt(0) == ' ')            
              theValue = theValue.substring(1, theValue.length);           
              return (theValue);    }
              // -->
