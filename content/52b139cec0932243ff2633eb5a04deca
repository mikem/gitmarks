areaval[7] = new Array("","---","---","ADL","AKL","BNE","CNS","CHC","DRW","GIS","OOL","HLZ","KAT","MRO","MEL","NAN","NPE","NSN","NPL","NOU","PMR","PPT","PER","ROT","SYD","TRG","TOU","WAG","WLG","WHK");
areatxt[7] = new Array("-Select City-                    ","---","---","Adelaide","Auckland","Brisbane","Cairns","Christchurch","Darwin","Gisborne","Gold Coast","Hamilton","Kaitaia","Masterton","Melbourne","Nadi","Napier","Nelson","New Plymouth","Noumea","Palmerston","Papeete","Perth","Rotorua","Sydney","Tauranga","Touho","Wanganui","Wellington","Whakatane");

	document.page_data.dest1.length = areaval[code].length;

	for (i=0; i< areatxt[code].length;i++) {
		document.page_data.dest1.options[i] = 
			new Option(areatxt[code][i],areaval[code][i]);
	}
	document.page_data.dest1.options[0].selected=true;
}

function Chgmonth() {
	for (i=0; i < document.page_data.dep_month.length; i++) {
		if (document.page_data.dep_month[i].selected == true) {
			Setdate(i);
			break;
		}
	} 
}

function pop_cal()
{
	sTop = (screen.height - 400) / 2;
	sLeft = (screen.width - 600) / 2;
	sParam = "scrollbars=1,resizable=yes,toolbar=0,location=0,directories=0,";
	sParam += "status=0,menubar=0,width=220,height=230,";
	sParam += "top=" + sTop + ",left=" + sLeft;
	window.open("/tabasa/date_select.htm","popcal",sParam);
	return false;
}

function Setdate(mcode) {
monthval = new Array("0","31","31","28","31","30","31","30","31","31","30","31","30");

	document.page_data.dep_day.length = parseInt(monthval[mcode])+1;
	document.page_data.dep_day.options[0] = new Option("-Date-","");
	for(i=1;i<=parseInt(monthval[mcode]);i++) {
		document.page_data.dep_day.options[i] =new Option(i,i);
	}
	document.page_data.dep_day.options[0].selected=true;
}

function jump() {
	if(document.page_data.dest1.value=="") {
		alert("Please choose cities");
		return false;
	}
	if(document.page_data.dep_month.value=="" || document.page_data.dep_day.value=="") {
		alert("Please be sure to choose a start day");
		return false;
	}
	document.page_data.gocmd.value="list";
	document.page_data.target="_blank";
	document.page_data.action="http://www.iace.co.jp/cgi-bin/iace_air2E.cgi";
	document.page_data.submit();
}
//-->
</script>
