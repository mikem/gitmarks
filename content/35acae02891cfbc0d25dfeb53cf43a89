<script>
$(document).ready(
		function() {
            message = "Search our catalogs..";
			$('form[action="http://search.usa.gov/search"]').submit(function(){
				obj = $('form[action="http://search.usa.gov/search"] input[name="query"]')
				if(obj.val()==message || obj.val()==''){
					obj.val("");
					return true;
				}
			});
			$('form[action="http://search.usa.gov/search"] input[name="query"]').focus(function(){
				if($(this).val()== message){
					$(this).val("");
				};
			});
			$('form[action="http://search.usa.gov/search"] input[name="query"]').blur(function(){
				if($(this).val()==''){
				    $(this).val(message);
				}
			});
		}
    );
</script>
